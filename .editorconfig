[*.cs]

# CS8618: O campo não anulável precisa conter um valor não nulo ao sair do construtor. Considere declará-lo como anulável.
dotnet_diagnostic.CS8618.severity = none

# CS8601: Possível atribuição de referência nula.
dotnet_diagnostic.CS8601.severity = silent

# CS8602: Desreferência de uma referência possivelmente nula.
dotnet_diagnostic.CS8602.severity = silent

# CS8619: A anulabilidade de tipos de referência no valor não corresponde ao tipo de destino.
dotnet_diagnostic.CS8619.severity = silent

# IDE0055: Corrigir a formatação
dotnet_diagnostic.IDE0055.severity = silent

# CS0108: O membro oculta o membro herdado; nova palavra-chave ausente
dotnet_diagnostic.CS0108.severity = silent
